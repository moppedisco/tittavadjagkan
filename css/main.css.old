body 						{ padding: 200px 0 0; /*border-left: 10px solid #D11250; */}
section 					{ position: relative;  }
header 						{ position: fixed; margin: 0 0 0 -21%; }
header + article.size3of4	{ width: 100%; }
ul 							{list-style:none;margin:0;padding:0;}

section.line 				{ padding: 0 0 0px 25%;  }
section 					{ opacity: 0.3; } 
._active 					{ opacity: 1; }
.logo 						{ position: fixed; left: 4%; top: 40px; /*font-family: Allan, cursive;*/  }
.logo img 					{ margin: 0 0 0 -10px; }

/* ====== Headings ====== */
h1, .h1{font-size:100%; font-weight:bold; font-style: normal; }
h2, .h2{font-size:100%; font-weight:normal; font-style: normal; }
h3, .h3{font-size:100%; font-weight:bold; font-style: normal; }
h4, .h4{font-size:100%; font-weight:normal; font-style: italic; }
h5, .h5{font-size:108%; font-weight:bold; font-style: normal; }
h6, .h6{font-size:108%; font-weight:normal; font-style: italic; }

.contentRow .unit 			{ position: relative; border-right: 1px dotted #666;border-left: 1px dotted #666; padding: 0 3%; } 

.unitWork { height: 300px; }
.unitIntro { height: 200px; }
.unitSkills { height: 250px; }

._active .contentRow .unit 	{ border-right-color: #ccc;border-left-color: #ccc; }
.contentRow .unit:first-child { }
.contentRow ul 				{ margin: 0 0 10px; }
.timeStamp { font-weight: bold; text-align: right; font-size: 85%; }


.size1of4 { width: 23%; margin: 0 1%; }
header.size1of4 { margin: 0 0 0 -21%;}
.icon-image:before {
	width: 13px;
	height: 13px; 
	background: url("http://twitter.github.com/bootstrap/assets/img/glyphicons-halflings.png") no-repeat;
	float: left;
	margin: 2px 4px 0 0;
	content: " ";
	opacity: 0.2;
}

.icon-star:before { background-position: -120px 0; opacity: 1; }
.icon-time:before { background-position: -48px -24px; }
.icon-film:before { background-position: -192px 0; }
.icon-tag:before { background-position: 0 -48px; }
.active .icon-film:before { background-position: -264px -72px; opacity: 1; -moz-opacity: 1;filter:alpha(opacity=1); }
.paused .icon-film:before { background-position: -287px -72px; }

.icon-info:before { display: none; }
.icon-info {
	width: 13px;
	height: 13px; 
	background: url("http://twitter.github.com/bootstrap/assets/img/glyphicons-halflings.png") no-repeat -121px -96px;
	display: inline-block;
	text-indent: -9999px;
	margin: 0 4px -1px 2px;	
	visibility: hidden;
	cursor: pointer;
}

._active .icon-info { visibility: visible; } 

.flashEmbed {
	position: relative;
	padding-bottom: 60.25%;
	padding-top: 28px;
	height: 0;
	overflow: hidden;
	margin: 0 1px 15px 0;
}

.flashEmbed object,
.flashEmbed embed,
.flashEmbed img {
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;

}
.youtube_item {
	margin: 0;
	border-top: 1px dotted #ddd;
	padding: 5px 0 2px;
	white-space: nowrap;
	width: 100%;
	overflow: hidden;
	text-overflow: ellipsis;
	margin: 0;
}

.youtube_item:first-child { border: none; }

.listBorder li { border-top: 1px dotted #ddd; padding: 5px 0 2px; }

.dateWrap { position: absolute; top: 0; left: 0;  }
.dateWrap span { display: block; }
.twitterText {  min-height: 85px; margin: 0; }

.tweetDate {
	display: block;
	font-weight: bold;
}

.tweetDate span {  }
.tweetDate .month {  }
.tweetDate .date { }

.tweetInfo 		{ overflow: hidden; display: none; }
._active .tweetInfo.show { display: block; }

.tweetInfo li 	{ 
	float: left;
	margin: 5px 15px 0 0;
	font-size: 85%;
	border-right: 1px solid #EEE;
	padding: 0 15px 0 0;
}
.tweetInfo span 		{ display: block;}

.tumblr_thumbnail { float: left; border: none; width: 75px; height: 75px; margin: 0 1px 1px 0;
  -webkit-transition: all 0.2s ease-out;  /* Saf3.2+, Chrome */
     -moz-transition: all 0.2s ease-out;  /* FF4+ */
      -ms-transition: all 0.2s ease-out;  /* IE10 */
       -o-transition: all 0.2s ease-out;  /* Opera 10.5+ */
          transition: all 0.2s ease-out;
}
.tumblr_thumbnail:hover { box-shadow: inset 4px 4px 0 #fff,inset -4px -4px 0 #fff; }
.tumblr_thumbnail img { border: none; float: left; }

